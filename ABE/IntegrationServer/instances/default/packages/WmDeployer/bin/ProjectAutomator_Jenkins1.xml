<?xml version="1.0" encoding="UTF-8" standalone="no"?><DeployerSpec exitOnError="true" sourceType="Repository">
	<DeployerServer>
		<host>deployerHost</host>
		<user>deployerUsername</user>
		<pwd>deployerPassword</pwd>
		<!-- <pwdHandle>$(PasswordHandle)</pwdHandle> -->
	</DeployerServer>
	<Environment>
		<Repository>
			<repalias name="projectRepoJenkins1">
				<type>FlatFile</type>
				<urlOrDirectory>repoTargetPath</urlOrDirectory>
				<createIndex>true</createIndex>
				<Test>true</Test>
			</repalias>
		</Repository>
		<IS>
			<isalias name="targetIS">
				<host>targetHost</host>
				<port>targetPort</port>
				<user>targetUsername</user>
				<pwd>targetPassword</pwd>
				<!-- <pwdHandle>$(PasswordHandle)</pwdHandle> -->
				<useSSL>targetUseSSL</useSSL>
				<version>targetWMVersion</version>
				<installDeployerResource>true</installDeployerResource>
				<Test>true</Test>
				<executeACL>Administrators</executeACL>
			</isalias>
		</IS>
	</Environment>
	<Projects projectPrefix="">
		<Project description="" name="projectJenkins1" overwrite="false" type="Repository">
			<ProjectProperties>
				<Property name="projectLocking">false</Property>
				<Property name="concurrentDeployment">true</Property>
				<Property name="ignoreMissingDependencies">false</Property>
				<Property name="isTransactionalDeployment">true</Property>
			</ProjectProperties>
			<DeploymentSet autoResolve="full" description="" name="Source" srcAlias="projectRepoJenkins1">
				<Composite displayName="NMBuildTest" name="NMBuildTest" srcAlias="projectRepoJenkins1" type="IS"/>
			</DeploymentSet>
			<DeploymentMap description="" name="Destination"/>
			<MapSetMapping mapName="Destination" setName="Source">
				<alias type="IS">targetIS</alias>
			</MapSetMapping>
			<DeploymentCandidate description="" mapName="Destination" name="myDeployment"/>
		</Project>
	</Projects>
</DeployerSpec>