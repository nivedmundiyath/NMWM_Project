    <tag>
        <name>ifLocked</name>
        <tagclass>com.webmethods.portal.taglib.meta.version.IfLockedTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>Operates like util:if tag,
            where util:then tag is evaluated if the specified thing is locked,
            and util:else tag is evaluated if it is not.</info>
        <attribute>
            <name>uri</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>view</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>ifVersioned</name>
        <tagclass>com.webmethods.portal.taglib.meta.version.IfVersionedTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>Operates like util:if tag,
            where util:then tag is evaluated if the specified thing is versioned,
            and util:else tag is evaluated if it is not.</info>
        <attribute>
            <name>uri</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>view</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>lockedBy</name>
        <tagclass>com.webmethods.portal.taglib.meta.version.LockedByTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>Prints the thing ID of the user who's locked the specified thing.</info>
        <attribute>
            <name>uri</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>view</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>lockedUntil</name>
        <tagclass>com.webmethods.portal.taglib.meta.version.LockedUntilTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>Prints the date the specified thing is locked until.</info>
